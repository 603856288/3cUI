/**container**/
page{
  background: #f5f5f5;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
  font-size:32rpx;
  color:#333;
} 

/**按钮**/
.btnNormal{
  margin:0 30rpx 30rpx 30rpx;
  background:#057bff;
  color:#fff;
  height:90rpx;
  line-height:90rpx;
  font-size:36rpx;
}
.btnNormal.button-hover {
  background:#0060cb;
}
/* 默认蓝色 */
.btnWhite{
  background: #fff;
  color:#057bff;
  height:88rpx;
  line-height:90rpx;
  border:1px solid #057bff;
}
.btnWhite.button-hover {
  background:#f0f8fd;
}
/* 白底蓝框 */
.btnFix{
  position:fixed;
  display:table;
  width:100%;
  bottom:0;
  left:0;
  background: #fff;
  z-index: 9;
}
.btnFix .btnView{
  display:table-cell;
  height:98rpx;
  line-height:98rpx;
}
.btnFix .l{
  width:60%;
  background:#fff;
  font-size: 34rpx;
  padding: 0 0 0 30rpx;
}
.btnFix .l .totalPrice{
    color:#ee6c39;
    font-size: 38rpx;
    padding-left: 10rpx;
}
.btnFix .r{
  width:40%;
  background:#057bff;
  font-size:34rpx;
  color:#fff;
  text-align: center;
  border-radius:0;
}
.btnFix .r.button-hover {
  background:#0060cb;
}
.btnfix .r::after{
  content:none;
}
/**btnFix固定底部两个按钮**/
.btnFix_single{
  position:fixed;
  width:100%;
  bottom:0;
  left:0;
  z-index: 9;
}
.btnFix_single button{
  width:100%;
  border-radius:0;
  font-size:34rpx;
  height:100rpx;
  line-height:100rpx;
  text-align: center;
}
.btnFix_single button::after{
  content:none;
}
.btnFix_single .bNormal{
  background:#057bff;
  color:#fff;
}
.btnFix_single .bNormal.button-hover {
  background:#0060cb;
}
.btnFix_single .bWhite{
  background: #fff;
  color:#057bff;
  border-top:1px solid #057bff;
  border-bottom:1px solid #057bff;
  height:98rpx;
  line-height:100rpx;
}
.btnFix_single .bWhite.button-hover {
  background:#f0f8fd;
}
/* 单个固定底部的按钮 */
.guideBtn{
  padding:10rpx 0;
  margin: 30rpx auto 0;
  min-width:240rpx;
  text-align: center;
}
.guideBtn_b{
  border:1rpx solid #0097ff;
  border-radius: 50rpx;
}
.guideBtn .call_icon{
  width:32rpx;
  height:30rpx;
  display: inline-block;
  vertical-align: middle;
}
.guideBtn .call_text{
  font-size:28rpx;
  color:#0097ff;
  vertical-align: middle;
  margin-left: 10rpx;
}
/* 联系客服按钮 */



/* checkboxBg */
.checkView {
  margin: 0 30rpx;
  padding: 40rpx 0;
}
.checkboxGroup {
  display: inline-block;
}
.checkbox {
  display: inline-block;
  font-size: 26rpx;
  color: #9b9b9b;
}
.openNotes {
  font-size: 26rpx;
  color: #057bff;
}
/* 原生checkbox */
.checkboxBg_on{
  display: inline-block;
  background:url(https://static.zhongan.com/website/open/assets/wp/3cMiniAppUI/images/checkboxBg.png) no-repeat 0 0;
  background-size:36rpx 94rpx;
  width:36rpx;
  height:36rpx;
  float:right;
  margin-left: 20rpx;
}
.checkboxBg{
  display: inline-block;
  background:url(https://static.zhongan.com/website/open/assets/wp/3cMiniAppUI/images/checkboxBg.png) no-repeat 0 -58rpx;
  background-size:36rpx 94rpx;
  width:36rpx;
  height:36rpx;
  float:right;
  margin-left: 20rpx;
}

/* tips特别标注 */
.tips_b {
  background: #fff7ed;
  line-height: 36rpx;
  border-radius: 10rpx;
  padding: 25rpx;
  text-align: justify;
  color: #ff7e20;
  font-size: 24rpx;
  margin:0 30rpx;
}
.tips_b view {
  margin-bottom: 10rpx;
  overflow: hidden;
}
.tips_b view text:first-child{
  width:2%;
}
.tips_b view text:last-child {
  width: 98%;
}
.tips_b view:last-child {
  margin-bottom: 0;
}

.tips_m{
  background:#fff7ed;
  line-height:60rpx;
  border-radius:10rpx;
  padding:0 0 0 25rpx;
}
.tips_m text{
  color:#ff7e20;
  font-size:26rpx;
}
/* 公共标题commonTitle */
.commonTitle{
  padding: 20rpx 30rpx;
  color:#999999;
  font-size: 28rpx;
}
/* 边框0.5px */
.borderLeft::before{
  content:'';
  width:1px;
  background: #e6e6e6;
  height:100%;
  position: absolute;
  transform:scale(0.5,1);
  -webkit-transform:scale(0.5,1);
  top: 0;
  left:0;
}
.borderRight::before{
  content:'';
  width:1px;
  background: #e6e6e6;
  height:100%;
  position: absolute;
  transform:scale(0.5,1);
  -webkit-transform:scale(0.5,1);
  top: 0;
  right:0;
}
.borderTop::before{
  content:'';
  width:100%;
  background: #e6e6e6;
  height:1px;
  position: absolute;
  transform:scale(1,0.5);
  -webkit-transform:scale(1,0.5);
  top: 0;
  left:0;
}
.borderBottom::before{
  content:'';
  width:100%;
  background: #e6e6e6;
  height:1px;
  position: absolute;
  transform:scale(1,0.5);
  -webkit-transform:scale(1,0.5);
  bottom: 0;
  left:0;
}

/* 箭头  右 */
.rightArrow::after{
  content:"";
  background:url('https://static.zhongan.com/website/open/assets/wp/3cMiniAppUI/images/iconArrow.png') no-repeat center;
    background-size:16rpx 28rpx;
  width:16rpx;
  height:28rpx;
  display:inline-block;
  position: absolute;
  top:50%;
  right:30rpx;
  transform:translate(0,-50%);
    -webkit-transform:translate(0,-50%);
}
/* 箭头  下*/
.downArrow::after{
  content:"";
  background:url('https://static.zhongan.com/website/open/assets/wp/3cMiniAppUI/images/iconArrowDown.png') no-repeat center;
    background-size:28rpx 16rpx;
  width:28rpx;
  height:16rpx;
  display:inline-block;
  position: absolute;
  top:50%;
  right:30rpx;
  transform:translate(0,-50%);
    -webkit-transform:translate(0,-50%);
}
/**表单1 uText**/
.uText{
  margin:0 0 0 30rpx;
  padding: 20rpx 0;
  position:relative;
}
.uText::before{
  content:'';
  width:100%;
  background: #e6e6e6;
  height:1px;
  position: absolute;
  transform:scale(1,0.5);
  -webkit-transform:scale(1,0.5);
  bottom: 0;
  left:0;
}
.uText.last::before{
  content:none;
}
.uText .key{
  display:inline-block;
  width:170rpx;
  font-size:32rpx;
  color:#333;
  vertical-align: middle;
}
.uText .value,.uText input{
  line-height:40rpx;
  display: inline-block;
  vertical-align: middle;
  width: 520rpx;
}
.uText .yzm{
  width:250rpx;
  font-size:28rpx;
  color:#057bff;
  position:absolute;
  right:0;
  top:0;
  height:100%;
  display:flex;
  align-items:center;
  justify-content:center;
  z-index:9;
}

/* 表单2 detailInfo */
.detailInfo{
  padding:0 0 0 30rpx;
  background:#fff;
  font-size:28rpx;
}
.detailInfo .uInfo{
  height:90rpx;
  line-height:90rpx;
  padding:0 30rpx 0 0;
  position: relative;
}
.detailInfo .uInfo::before{
  content:'';
  width:100%;
  background: #e6e6e6;
  height:1px;
  position: absolute;
  transform:scale(1,0.5);
  -webkit-transform:scale(1,0.5);
  bottom: 0;
  left:0;
}
.detailInfo .uInfo.last::before{
  content:none;
}

.detailInfo .address{
  display:table;
  padding:30rpx 0;
  line-height:45rpx;
  width:100%;
  height:auto;
}
.detailInfo .address-fl{
  display:table-cell;
  width:30%;
}
.detailInfo .address-fr{
  display:table-cell;
  width:70%;
  word-break:break-all;
  text-align:right;
  padding-right:30rpx;
}

/**表单3 breakList**/
.breakList{
  padding:30rpx;
  background: #fff;
}
.breakList .phoneType{
  color:#999999;
  font-size:26rpx;
}
.breakList .breakItem{
  margin:30rpx 0 0 0;
  font-size:28rpx;
  color:#333;
}

/* 进度条 */
.stepList{
  padding:30rpx 0 0 30rpx;
  background: #fff;
}
.step{
  position:relative;
  padding:0 0 35rpx 40rpx;
  line-height:31rpx;
}
.step::before{
  content:'';
  display:inline-block;
  position:absolute;
  width:1px;
  height:100%;
  left:15rpx;
  top:0;
  background:#e5e5e5;
}
.step::after{
  content:'';
  display:inline-block;
  position:absolute;
  width:20rpx;
  height:20rpx;
  left:6rpx;
  top:6rpx;
  background:#cccccc;
  border-radius:100%;
}
.step .t1{
  line-height: 1;
  color:#999;
  font-size:28rpx;
}
.step .t2{
  color:#999;
  font-size:26rpx;
  margin-top:10rpx;
}
.step-on{
  position:relative;
  padding:0 0 35rpx 40rpx;
  line-height:32rpx;
}
.step-on::after{
  width:26rpx;
  height:26rpx;
  left:-2px;
  top:0;
  background:#0097ff;
  border:6rpx solid #b8d9ff; 
}
.step-on .t1{
  color:#0078ff;
}
.step-on .t2{
  color:#0078ff;
}
.look-btn {
    color:#0078ff;
    font-size: 28rpx;
    position: absolute;
    right: 30rpx;
    top:0;
}

/**评分stars**/
.comment-result{
  padding:30rpx;
}
.comment-result .starRow{
  height:56rpx;
  margin-bottom: 30rpx;
}
.comment-result .starTitle{
  color:#666666;
  font-size:28rpx;
  line-height:56rpx;  
}
.comment-result .star-image {
  position: relative;
  width: 58rpx;
  height: 56rpx;
  margin:0 35rpx 0 0;
}
.comment-result .star-item {
  position: absolute;
  top: 0;
  left:0;
  width: 58rpx;
  height: 56rpx;
}
.comment-result .starRow .star-image:last-child{
  margin-right: 0;
}

/** 通用遮罩mask **/
.mask{
  width:100%;
  height:100%;
  background:#999;
  filter:alpha(opacity=80);  
  -moz-opacity:0.8;  
  -khtml-opacity: 0.8;  
  opacity: 0.8;  
  top:0;
  left:0;
  z-index:991;
  position:fixed;
}

/**头部固定线条**/
.bgLine{
  background:#b1b1b1;
  height:1rpx;
  width:100%;
  position:fixed;
  top:0;
  left:0;
  z-index:9;
}
